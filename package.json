{
  "name": "vue-metro-tile",
  "version": "1.0.0",
  "description": "A windows 10 metro tile component in vue",
  "main": "dist/vue-metro-tile.common.js",
  "unpkg": "dist/vue-metro-tile.umd.min.js",
  "author": "Yuan Fu <fuyuanx@gmail.com>",
  "keywords": [
    "metro",
    "tile",
    "windows"
  ],
  "files": [
    "dist/",
    "README.md"
  ],
  "homepage": "https://github.com/yuanfux/vue-metro-tile#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuanfux/vue-metro-tile.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest docs",
    "build-lib": "vue-cli-service build --target lib ./src/components/MetroTile.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "vue-grid-layout": "^2.2.0",
    "vue-svg-loader": "^0.9.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "MIT"
}
